{
  "name": "backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "prisma-generate": "npx prisma generate",
    "prisma-migrate-init": "npx prisma migrate dev --name init",
    "prisma-migrate-dev": "npx prisma migrate dev",
    "prisma-connect": "npm run prisma-generate && npm run prisma-migrate-init && npm run prisma-migrate-dev",
    "test": "jest --testPathPattern=user.test.ts",
    "dev-run": "npm run build && node dist/entrypoint.js",
    "dev-test": "npm run build && npm run test"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seeds/seed.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@types/jsonwebtoken": "^9.0.6",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.10.2"
  }
}
